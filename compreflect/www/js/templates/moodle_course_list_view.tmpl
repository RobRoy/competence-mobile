<% _.each(courses, function(course){ %>
    <li class='moodle-course'  courseid='<%= course.get('id') %>'><h3>
      <a href="#moodle/<%= course.get('id') %>" data-role="button" data-iconpos="right" data-icon="arrow-darkblue" data-transition="slidefade">
        <%= course.get('fullname') %> (<%= course.get('shortname') %>)
      </a></h3>
    </li>
<%});%>