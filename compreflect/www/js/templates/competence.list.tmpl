<ul id="competencelist"  data-role="listview" data-inset="true">
  <% if (store.get('up.session.compcontext') == "coursecompetences") { %>
    <li class="courseHeadline" style="background: #014260; min-height: 2em; color: #fff; text-align: center; font-family: georgia,serif; font-style: italic; letter-spacing: 1px; margin-bottom: 5px; padding-top: 10px;">
  		<%= store.get('up.session.courseName') %>
  	</li>
  <% } %>
  <% _.each(competences, function(competence) { %>
  	<li>
  		<a href="#competences/<% print(encodeURIComponent(this)); %>/<% print(encodeURIComponent(this)); %>"><%= competences[competence] %></a>
  	</li>
  <% }) %>
</ul>