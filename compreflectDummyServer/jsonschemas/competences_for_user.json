{
  "title": "comptenceslist",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "the list of competences for a single course",
  "properties": {
    "competences": {
      "description": "the comptences list",
      "type": "array",
      "items": {
        "description": "object representing a single competence",
        "type": "object",
        "properties": {
          "id": {
            "description": "the competence id",
            "type": "integer"
          },
          "title": {
            "description": "the string representation of a competence",
            "type": "string",
            "ipsum": "title"
          }
        }
      },
      "minItems": 0,
      "uniqueItems": true
    }
  }
}